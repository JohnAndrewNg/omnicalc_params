<h1>Random Calculation</h1>

<form action="/random/results">
  <div>
    <label for="users_min">
      Minimum
    </label>
    <input id="users_min" type="text" name="min" placeholder="E.g. 1.5">
  </div>
  <div>
    <label for="users_max">
      Maximum
    </label>
    <input id="users_max" type="text" name="max" placeholder="E.g. 4.5">
  </div>

  <button>Pick random number</button>

</form>
